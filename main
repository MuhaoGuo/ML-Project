from sklearn.ensemble import RandomForestClassifier
import pandas as pd
# import Logistic_Regression
# import svc

train = pd.read_csv("train.csv", index_col=0)
test = pd.read_csv("test.csv", index_col=0)

#####  separate x and yï¼š

y_test = test.loc[:,'ConvertedComp']
x_test = test.drop(['MainBranch','ConvertedComp'],axis=1)
y_train = train.loc[:,'ConvertedComp']
x_train = train.drop(['MainBranch','ConvertedComp'],axis=1)

def main():
    clf = RandomForestClassifier(n_estimators=800, bootstrap=True, max_features=9)
    clf.fit(x_train, y_train)
    score_test = clf.score(x_test, y_test)
    print("Random forest algorithm: the predit score is\n" + str(score_test))

main()
